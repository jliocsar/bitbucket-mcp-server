element := <T < Symbol>({ description: tag }: T) -> (...content: string[]) ->
  `<${tag}>${content.join ""}</${tag}>`

div := element :div
html := element :html
body := element :body
head := element :head
title := element :title
p := element :p

(html
  (head (title "Hello"))
  (body
    (div
      (p "Hello")
      (p "World"))))